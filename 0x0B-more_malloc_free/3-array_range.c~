#include "holberton.h"
#include <stdlib.h>

/**
 * name - description
 *
 * @param: description
 * Return: description
 */

int *array_range(int min, int max)
{
	int *array;
	int i;

	if (min > max)
		return (NULL);
	array = malloc((max - min) * sizeof(int));
	if (array == NULL)
		return (NULL);
	i = 0;
	while (min < max)
		array[i++] = min++;
	return (array);
}
